        "X_test_class_0 = pd.DataFrame(X_test[y_test == 0])\n",
        "y_test_class_0 = pd.Series(y_test[y_test == 0])\n",
        "\n",
        "# Select all samples from class 1 in the test set\n",
        "X_test_class_1 = pd.DataFrame(X_test[y_test == 1])\n",
